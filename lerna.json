{
  "version": "independent",
  "npmClient": "npm",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "allowBranch": "master",
      "conventionalCommits": true,
      "exact": true,
      "ignoreChanges": ["ignored-file", "**/__tests__/**", "**/*.md"]
    },
    "version": {
      "message": "Publish %s (ignore)"
    }
  }
}
